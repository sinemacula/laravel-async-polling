{
    "name": "sinemacula/laravel-async-polling",
    "type": "library",
    "description": "A simple polling trait, designed to extend Laravel's queued jobs with asynchronous polling",
    "keywords": [
        "sine macula",
        "laravel",
        "queues",
        "polling"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ben Carey",
            "email": "bdmc@sinemacula.co.uk"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "SineMacula\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
